            <Id>
                <TypeId>ConveyorSorter</TypeId>
                <SubtypeId>MCRNRetractablePDC_Block</SubtypeId>
            </Id>
            <DisplayName>40mm Nariman Retractable PDC [MCRN]</DisplayName>
            <Description>A good reliable mass produced MCRN PDC. It is commonly used on most MCRN warships</Description>
	    <GuiVisible>true</GuiVisible>
	    <Icon>Textures\GUI\Icons\Cubes\MCRNRetractablePDC.dds</Icon>
            <CubeSize>Large</CubeSize>
            <BlockTopology>TriangleMesh</BlockTopology>
            <Size x="1" y="2" z="2"/>
	    <ModelOffset x="0" y="0" z="0" />
            <Model>Models\Cubes\large\MCRNRetractablePDC.mwm</Model>
            <Components>
			<Component Subtype="SteelPlate" Count="10" />
			<Component Subtype="Construction" Count="30" />
			<Component Subtype="MetalGrid" Count="15" />
			<Component Subtype="SmallTube" Count="6" />
			<Component Subtype="Motor" Count="8" />
			<Component Subtype="Computer" Count="10" />
			<Component Subtype="Construction" Count="10" />
                        <Component Subtype="MetalGrid" Count="15" />
			<Component Subtype="SteelPlate" Count="30" />
	    </Components>
            <CriticalComponent Index="0" Subtype="Computer"/>
			<MountPoints>
				<MountPoint Side="Front" StartX="0.00" StartY="0.00" EndX="1.00" EndY="1.00" PressurizedWhenOpen="true"/>
				<MountPoint Side="Back" StartX="0.00" StartY="0.00" EndX="1.00" EndY="1.00" PressurizedWhenOpen="true"/>
				<MountPoint Side="Left" StartX="0.00" StartY="0.00" EndX="2.00" EndY="1.00" PressurizedWhenOpen="true"/>
